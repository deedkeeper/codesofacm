//website http://acm.cup.edu.cn/newsubmitpage.php?tid=1&pid=5
#include <stdio.h>
int main()
{
    int a[100],b[100],c[100],m;
    int i,j,k;
    scanf("%d",&m);
    for(i=0;i<m;i++)
{    scanf("%d",&a[i]);
    for(j=0;j<a[i];j++)
 {   scanf("%d",&b[j]);
 }
 
    
}   
  for(i=0;i<m;i++)
{    c[i]=0;
    for(j=0;j<a[i];j++)
 {   c[i]=c[i]+b[j];
 }
}     
for(i=0;i<m;i++)
{    printf("%d\n",c[i]);
 }
 return 0;
}
